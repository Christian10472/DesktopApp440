      <TextField fx:id="EmailUser" layoutX="15.0" layoutY="206.0" prefHeight="26.0" prefWidth="249.0" promptText="example@example.com" />
      <TextField fx:id="PasswordUser" layoutX="15.0" layoutY="268.0" prefHeight="26.0" prefWidth="250.0" promptText="Password" />
      <Label layoutX="15.0" layoutY="241.0" prefHeight="27.0" prefWidth="123.0" text="Password">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label layoutX="15.0" layoutY="25.0" prefHeight="27.0" prefWidth="123.0" text="Full Name">

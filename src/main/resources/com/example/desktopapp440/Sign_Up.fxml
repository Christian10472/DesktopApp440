<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="661.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.desktopapp440.SIgnUpController">
   <children>
      <Button fx:id="SignUp" disable="true" layoutX="273.0" layoutY="346.0" mnemonicParsing="false" onAction="#onSignUpClick" text="Sign me up!" />
      <Label layoutX="15.0" layoutY="179.0" prefHeight="19.0" prefWidth="123.0" text="Email Address">
         <font>
            <Font size="18.0" />
